{
  "name": "shape-shifter-editor-frontend",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "build:skip-check": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:codegen": "playwright codegen http://localhost:5173",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "generate:favicon": "node scripts/generate-favicon.js"
  },
  "dependencies": {
    "@guolao/vue-monaco-editor": "^1.5.6",
    "@vueuse/core": "^11.3.0",
    "ag-grid-community": "^32.3.3",
    "ag-grid-vue3": "^32.3.3",
    "axios": "^1.7.9",
    "cytoscape": "^3.33.1",
    "cytoscape-cose-bilkent": "^4.1.0",
    "cytoscape-dagre": "^2.5.0",
    "d3": "^7.9.0",
    "js-yaml": "^4.1.0",
    "monaco-editor": "^0.52.0",
    "monaco-yaml": "^5.1.1",
    "pinia": "^2.3.0",
    "vee-validate": "^4.14.7",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vuetify": "^3.11.4",
    "yaml": "^2.3.4",
    "zod": "^3.24.1"
